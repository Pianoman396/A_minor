{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": [2, "always"],
        "no-extra-semi": 2,
        "quotes": [2, "double", {"allowTemplateLiterals": true}],
        "eqeqeq": [2, "always"],
        "curly": 2
    }
}